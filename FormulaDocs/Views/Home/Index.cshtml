@model FormulaDocs.Models.PublicRelation
@{
    ViewBag.Title = "หน้าแรก";
}


<div class="container h-100">
    <div class="row h-100 justify-content-center align-items-center">
        <div class="col-12">
            <div class="card">
                <div class="card-body">
                    <h2>ยินดีต้อนรับ</h2>                   
                        <p>@Html.DisplayName(Model.Date.ToString())</p>
                        <p>@Html.DisplayName(Model.Name)</p>
                        <p>@Html.DisplayName(Model.Description)</p>
                        <p>@Html.DisplayName(Model.Creator)</p>                   
                </div>
            </div>
        </div>
    </div>
</div>